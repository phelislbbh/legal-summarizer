\par {\listtext\pard\plain\insrsid6707614\charrsid15889768 \hich\af0\dbch\af0\loch\f0 1.\tab}}\pard \qj \fi-720\li1080\ri0\sl360\slmult1\widctlpar\jclisttab\tx1080\aspalpha\aspnum\faauto\ls1\adjustright\rin0\lin1080\itap0\pararsid15889768 {
